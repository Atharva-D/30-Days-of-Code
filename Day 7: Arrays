N = int(input())
for i in range(0,N):
  S = input()
  y = int("0")
  even = []
  odd = []
  for i in S:
      #print (i)
      if y % 2 == 0:
        even.append(i)
      else:
        odd.append(i)
      y += 1
  print(*even,sep='',end='')
  print(" " , end = '')
  print(*odd,sep='',end='')
  print("")
